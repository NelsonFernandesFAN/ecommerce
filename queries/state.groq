*[_type=="state"&&code=="NJ"]{
	name,
	code,
	"layout": layout[]{
		status,
		section->{
			title,
			type,
			type =='showcase-card'=>{
				draw->{
					name,
					"slug": game_slug.current,
					description,
					"image": featuredImage.asset->url
				}
			},
			type=='aggregate-card'||type=='carousel'=>{
				"draws" = draw[]->{
					name,
					slug,
					description,
					"image": featuredImage.asset->url
				}
				}
			},
		}
	}