*[_type=="state"&&state_code=="NJ"]{
	state_name,
	state_code,
	"lobby": lobby[]{
		widget_env,
		widget->{
			widget_title,
			widget_type,
			widget_type=='showcase-card'=>{
				game->{
					game_name,
					"game_slug": game_slug.current,
					game_description,
					"game_image": game_image.asset->url
				}
			},
			widget_type=='aggregate-card'||widget_type=='carousel'=>{
					game_name,
					game_slug,
					game_description,
					"image": game_image.asset->url
				}
			},
		}
	}
}